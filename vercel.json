{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "https://jonahsnider.dev/keybase.txt",
			"destination": "/keybase.txt"
		}
	],
	"redirects": [
		{
			"source": "https://jonahsnider.dev/:path*",
			"destination": "https://github.com/jonahsnider/:path*"
		},
		{
			"source": "/resume",
			"destination": "https://drive.google.com/file/d/11FOwQyPz1Qy8vxHyJjN_Oaxh2rHGRTzW/view?usp=sharing"
		},
		{
			"source": "/linkedin",
			"destination": "https://linkedin.com/in/jonahsnider/"
		},
		{
			"source": "/keybase",
			"destination": "https://keybase.io/jonahsnider/"
		},
		{
			"source": "/email",
			"destination": "mailto:jonah@jonahsnider.com"
		},
		{
			"source": "/gh/:path*",
			"destination": "https://github.com/jonahsnider/:path*"
		},
		{
			"source": "/github/:path*",
			"destination": "https://github.com/jonahsnider/:path*"
		}
	]
}
